{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "import/prefer-default-export": "off",
        "import/no-mutable-exports": "off",
        "no-new-func": "off",
        "no-console": "off",
        "import/no-cycle": "off",
         "quotes": [
             "error",
             "single"
        ]
    }
}
